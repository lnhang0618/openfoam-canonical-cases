FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "450";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 0.0001;

boundaryField
{
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    bottomWall
    {
        type            nutLowReWallFunction;
        value           $internalField;
    }
    topWall
    {
        type            nutLowReWallFunction;
        value           $internalField;
    }
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
}


// ************************************************************************* //
